---
layout: null
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Taylor Jadin's Posts</title>
	<link href="http://taylorjadin.github.io/posts/atom.xml" rel="self"/>
	<link href="http://taylorjadin.github.io/posts"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://taylorjadin.github.io/posts</id>
	<author>
		<name>Taylor Jadin</name>
		<email>taylor@jadin.me</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="http://hankquinlan.github.io{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://hankquinlan.github.io/{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
